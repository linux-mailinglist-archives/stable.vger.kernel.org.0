    aarch64:
      make options: -j30 INSTALL_MOD_STRIP=3D1 targz-pkg

    ppc64le:
      make options: -j30 INSTALL_MOD_STRIP=3D1 targz-pkg

    s390x:
      make options: -j30 INSTALL_MOD_STRIP=3D1 targz-pkg

